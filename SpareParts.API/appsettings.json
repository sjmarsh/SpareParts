{
  "ConnectionStrings": {
    "SparePartsDbConnection": "Server=.\\SQLEXPRESS;Database=SpareParts;Trusted_Connection=True"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "Microsoft.EntityFrameworkCore.Database.Command": "Information"
      }
    },
    //"Filter": [
    //  {
    //    "Name": "ByExcluding",
    //    "Args": { "expression": "@mt = 'An unhandled exception has occurred while executing the request.'" }
    //  }
    //],
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "./logs/log-.txt",
          "rollingInterval": "Month"
        }
      }
    ]
  },
  "AllowedHosts": "*"
}
